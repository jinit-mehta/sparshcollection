{
    "name": "sparsh/collection",
    "description": "Sparsh Collection E-commerce",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "codeigniter4/framework": "^4.3",
        "codeigniter4/shield": "^1.0",
        "codeigniter4/settings": "^2.0",
        "dompdf/dompdf": "^3.0",
        "razorpay/razorpay": "^2.9",
        "google/apiclient": "^2.15",
        "orhanerday/open-ai": "^4.8"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Config\\": "app/Config/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.2"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}